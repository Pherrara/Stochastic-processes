    "import numpy as np\n",
    "import random\n",
    "\n",
    "N = int(input())\n",
    "a = np.arange(N)\n",
    "a += 1\n",
    "b = np.zeros(N)\n",
    "ae = 0\n",
    "\n",
    "for i in range(N):\n",
    "    ae = random.randint(1,N)\n",
    "    b[i] = ae\n",
    "\n",
    "print(b)"
